{
	"version": 2,
	"builds": [
		{
			"src": "_release/bin/SaveUpApi.exe",
			"use": "now-static-bin",
			"config": {
				"port": 8080
			}
		}
	],
	"routes": [
		{
			"src": "^/.*",
			"dest": "/SaveUpApi.exe"
		}
	],
	"env": {
		"CLIENT_HOST": "@saveup-client-host",
		"DATABASE_URL": "@saveup-database-url",
		"SYSTEM_EMAIL": "@saveup-system-email",
		"SYSTEM_JWT": "@saveup-system-jwt"
	}
}
