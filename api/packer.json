{
	"variables": {
		"api_access_key": "{{env `SCALEWAY_ORGANIZATION`}}",
		"api_token": "{{env `SCALEWAY_TOKEN`}}",
		"home": "{{env `HOME`}}"
	},
	"sensitive-variables": [
		"api_access_key",
		"api_token"
	],
	"builders": [
		{
			"api_access_key": "{{ user `api_access_key` }}",
			"api_token": "{{ user `api_token` }}",
			"server_name": "save-up-api",
			"image": "90d69ee1-2049-49ef-819c-dce675037477",
			"image_name": "save-up-api",
			"type": "scaleway",
			"region": "ams1",
			"commercial_type": "START1-XS",
			"ssh_private_key_file": "{{ user `home` }}/.ssh/id_rsa",
			"ssh_username": "root"
		}
	],
	"provisioners": [
		{
			"type": "file",
			"source": "packer/api.service",
			"destination": "/lib/systemd/system//api.service"
		}
	]
}
