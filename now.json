{
	"version": 2,
	"builds": [
		{
			"src": "api/dist/bin/api.exe",
			"use": "now-static-bin",
			"config": {
				"port": 4010
			}
		}
	],
	"env": {
		"API_SECRET": "@saveup-secret-key",
		"CLIENT_HOST": "@saveup-client-host",
		"DATABASE_URL": "@saveup-database-url",
		"ROCKET_ENV": "prod",
		"ROCKET_PORT": "4010",
		"SYSTEM_EMAIL": "@saveup-system-email",
		"SYSTEM_JWT": "@saveup-system-jwt"
	}
}
